---
import PublicLayout from '@/layouts/PublicLayout.astro';

const robots = ["index", "follow"];
const title = "Iniciar Sesión";
const description = "Accede a tu cuenta en TrinUp y continúa tu camino hacia una vida más saludable. Gestiona tu progreso en nutrición, bienestar y hábitos positivos.";
const keywords = ["iniciar sesión", "login", "acceder a cuenta", "TrinUp", "salud y bienestar", "nutrición", "hábitos saludables", "bienestar personal"];
const url = "https://trinup-frontend.vercel.app";
const imageOG = "/card-twt.webp";
const imagetwt = "/card-twt.webp";
---

<PublicLayout {robots} {title} {description} {keywords} {url} {imageOG} {imagetwt}>
    <section>
        <h1>Login</h1>
    </section>
</PublicLayout>
